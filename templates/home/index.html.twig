{% extends 'layout.html.twig' %}

{% block title %}{{ parent() }}{% endblock %}
    {% block stylesheets %}
        {{ parent() }}
        {{ encore_entry_link_tags('home') }}
    {% endblock %}

{% block body %}
    <h1>IDEOZ</h1>
    <section class="service">
        <h2 class="text-center">Mes services</h2>
        <div class="container-fluid">
            <div class="row mx-0 justify-content-center">
                {% for service in services %}
                    <a href="" class="card p-3" style="width: 22rem;">
                        <div class="card-title my-3">
                            <h3>
                                <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-bookmarks-fill"
                                     fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                                    <path fill-rule="evenodd"
                                          d="M2 4a2 2 0 0 1 2-2h6a2 2 0 0 1 2 2v11.5a.5.5 0 0 1-.777.416L7 13.101l-4.223 2.815A.5.5 0 0 1 2 15.5V4z"/>
                                    <path fill-rule="evenodd"
                                          d="M4.268 1H12a1 1 0 0 1 1 1v11.768l.223.148A.5.5 0 0 0 14 13.5V2a2 2 0 0 0-2-2H6a2 2 0 0 0-1.732 1z"/>
                                </svg>
                                {{ service.name }}
                            </h3>
                        </div>
                        <div class="card-body d-flex flex-column justify-content-between mb-3">
                            <p class="card-text">{{ service.abstract }}</p>
                            <div class="align-self-end mr-4">
                                <svg width="2em" height="2em" viewBox="0 0 16 16" class="bi bi-arrow-right-circle"
                                     fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                                    <path fill-rule="evenodd"
                                          d="M8 15A7 7 0 1 0 8 1a7 7 0 0 0 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z"/>
                                    <path fill-rule="evenodd"
                                          d="M4 8a.5.5 0 0 0 .5.5h5.793l-2.147 2.146a.5.5 0 0 0 .708.708l3-3a.5.5 0 0 0 0-.708l-3-3a.5.5 0 1 0-.708.708L10.293 7.5H4.5A.5.5 0 0 0 4 8z"/>
                                </svg>
                            </div>
                        </div>
                    </a>
                {% endfor %}
            </div>
        </div>
    </section>
    <section class="testimony">
        <h2 class="text-center">Témoignages</h2>
        <div class="container-fluid">
            <div class="row mx-0 justify-content-center">
                {% for testimony in testimonies %}
                    <figure class="d-flex flex-column align-items-center m-5">
                        <img class="img-figure" src="{{ testimony.ecosystem.logo }}" alt="logo ecosystem">
                        <h3 class="my-3 text-center">{{ testimony.ecosystem.name }}</h3>
                        <figcaption>
                            <img src="data:image/svg+xml;base64,"
                                 alt="quote" class="img pb-2"/>
                            {{ testimony.content }}
                            <img src="data:image/svg+xml;base64,"
                            alt="quote" class="img pb-2"/>
                        </figcaption>
                    </figure>
                {% endfor %}
            </div>
        </div>
        <div class="d-flex justify-content-center">
            <a href="#" class="font-weight-bold text-decoration-none text-dark btn-lg rounded-pill my-4 px-5 py-3">Tous
                les témoignages</a>
        </div>
    </section>
    <section class="ecosystem">
        <h2 class="text-center">Ecosystème</h2>
        <h3 class="text-center">Clients</h3>
        <div class="scrolling-wrapper client text-center mx-md-5">
            {% for client in clients %}
                <div class="logo">
                    <a href="{{ client.url }}" target="_blank">
                        <img src="{{ client.logo }}" alt="{{ client.name }}">
                    </a>
                </div>
            {% endfor %}
        </div>
        <h3 class="text-center">Partenaires</h3>
        <div class="scrolling-wrapper partner text-center mx-md-5">
            {% for partner in partners %}
                <div class="logo">
                    <a href="{{ partner.url }}" target="_blank">
                        <img src="{{ partner.logo }}" alt="{{ partner.name }}">
                    </a>
                </div>
            {% endfor %}
        </div>
    </section>
{% endblock %}
